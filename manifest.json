{

  "manifest_version": 3,
  "name": "Thingiverse URL modifier",
  "version": "1.0",

  "description": "Redirects a thingiverse ../file path to ../zip",


  "permissions": [
    "tabs",
    "scripting"
  ],

  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],

  "icons": {
    "48": "icons/t-48.png",
    "64": "icons/t-64.png",
    "128":"icons/t-128.png"
  },

  "background": {
    "service_worker": "background.js"
  }

}